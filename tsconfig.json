{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "target": "es5",
    "jsx": "react",
    "lib": ["dom", "es6"],
    "baseUrl": "./",
    "paths": {
      "@/pages/*": ["frontend/pages/*"],
      "@/services/*": ["service/*"]
    }
  },
  "include": ["frontend", "backend"],
  "compileOnSave": false
}
